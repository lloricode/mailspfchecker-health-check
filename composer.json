{
    "name": "lloricode/mailspfchecker-health-check",
    "description": "This is my package mailspfchecker-health-check",
    "keywords": [
        "lloricode",
        "laravel",
        "mailspfchecker-health-check"
    ],
    "homepage": "https://github.com/lloricode/mailspfchecker-health-check",
    "license": "MIT",
    "authors": [
        {
            "name": "Lloric Mayuga Garcia",
            "email": "lloricode@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "dietercoopman/mailspfchecker": "^1.0.3",
        "illuminate/contracts": "^8.83.23|^9.27",
        "spatie/laravel-health": "^1.14.2",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^5.11|^6.0",
        "nunomaduro/larastan": "^1.0.3|^2.0.1",
        "orchestra/testbench": "^6.25|^7.7",
        "pestphp/pest": "^1.22.1",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.1.1",
        "phpunit/phpunit": "^9.5.24",
        "spatie/laravel-ray": "^1.30"
    },
    "autoload": {
        "psr-4": {
            "Lloricode\\MailspfcheckerHealthCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lloricode\\MailspfcheckerHealthCheck\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
